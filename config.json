{
  "input": [
    {
      "type": "maxmindGeoLite2CountryCSV",
      "action": "add",
      "args": {
        "country": "./geolite2/GeoLite2-Country-Locations-en.csv",
        "ipv4": "./geolite2/GeoLite2-Country-Blocks-IPv4.csv",
        "ipv6": "./geolite2/GeoLite2-Country-Blocks-IPv6.csv"
      }
    },
    {
      "type": "cutter",
      "action": "remove",
      "args": {
        "wantedList": ["cn"]
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "cn",
        "uri": "https://raw.githubusercontent.com/dl123100/clash-geoip/master/IPs/ChinaMax_IP.txt"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "lanip",
        "uri": "https://raw.githubusercontent.com/dl123100/clash-geoip/master/IPs/Lan_IP.txt"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "google",
        "uri": "https://raw.githubusercontent.com/Loyalsoldier/geoip/release/text/google.txt"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "apple",
        "uri": "https://raw.githubusercontent.com/Rafeal-Ma/My-IP/main/Apple"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "cn_bing",
        "uri": "https://raw.githubusercontent.com/dl123100/clash-geoip/master/IPs/CN_Bing_IP.txt"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "github",
        "uri": "https://raw.githubusercontent.com/dl123100/clash-geoip/master/IPs/Github_IP.txt"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "telegram",
        "uri": "https://core.telegram.org/resources/cidr.txt"
      }
    }
  ],
  "output": [
    {
      "type": "v2rayGeoIPDat",
      "action": "output",
      "args": {
        "outputName": "geoip.dat",
        "wantedList": ["cn", "lanip", "telegram"]
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "output",
      "args": {
        "outputName": "Country.mmdb",
        "wantedList": ["cn", "lanip", "telegram", "google", "apple", "cn_bing", "github"]
      }
    },
    {
      "type": "text",
      "action": "output"
    }
  ]
}
